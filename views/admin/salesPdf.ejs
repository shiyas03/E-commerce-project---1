    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.2/css/jquery.dataTables.min.css">

    <title>E Commerce</title>


</head>

<body>
    

    <div class="container">
        <form action="" method="post">
            <div class="row justify-content-end ">
                <div class="col-3">
                    <label for="html5-text-input" class="col col-form-label">From: <%= start %></label>
                </div>
                <div class="col-3">
                    <label for="html5-text-input" class="col col-form-label">To: <%= end %></label>
                </div>
            </div>

            <!--Products card-->
            <div class="bg-light mt-3">
                <div class="row justify-content-center">
                    <div class="g-3 col-12">
                        <table class="table table-bordered border table-responsive">
                            <thead>
                                <tr>
                                    <th scope="col">No</th>
                                    <th scope="col">User</th>
                                    <th scope="col">Product</th>
                                    <th scope="col">Quantity</th>
                                    <th scope="col">Price</th>
                                    <th scope="col">Date (dd-mm-yy)</th>
                                    <th scope="col">Payment</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for(var i = 0; i < data.length; i++){ %>
                                    <tr>
                                        <td>
                                            <%= i+1 %>
                                        </td>
                                        <td>
                                            <%= data[i].userId.userName %>
                                        </td>
                                        <td>
                                            <%= data[i].productId.productName %>
                                        </td>
                                        <td>
                                            <%= data[i].orderedQuantity %>
                                        </td>
                                        <td>
                                            <%= data[i].totalSalePrice %>
                                        </td>
                                        <td>
                                            <%= dates[i].newDate %>
                                        </td>
                                        <td>
                                            <%= data[i].paymentMethod %>
                                        </td>
                                    </tr>
                                    <% } %>
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </form>
    </div>

</body>

